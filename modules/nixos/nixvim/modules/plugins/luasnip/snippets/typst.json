{
  "New Japanese learning note": {
    "prefix": "jnote",
    "body": [
      "#import \"template.typ\": *",
      "",
      "#let today = datetime(year: ${1:year}, month: ${2:month}, day: ${3:day})",
      "",
      "#let primary-colour = (",
      "  red,",
      "  orange,",
      "  yellow,",
      "  green,",
      "  blue,",
      "  purple,",
      "  fuchsia,",
      ").at(today.weekday() - 1)",
      "#let secondary-colour = secondary-colour(primary-colour)",
      "#let frame = frame.with(primary-colour: primary-colour)",
      "#let trivia = trivia.with(primary-colour: primary-colour)",
      "#show: template.with(primary-colour: primary-colour, title-font: \"Jiyucho\")",
      "",
      "= #today.display(\"[year]年[month]月[day]日\")",
      "",
      "${4:body}"
    ]
  },
  "New Japanese phrase": {
    "prefix": "jphrase",
    "body": [
      "+ #furigana[${3:reading}][${2:phrase_sep}]<${1:phrase}>",
      "",
      "  - #highlight[Meaning]: ${4:meaning}",
      "",
      "  - #highlight[Kanji]:",
      "",
      "    ${5:kanji} (${6:kanji_vn}): ${7:kanji_meaning}"
    ]
  }
}
